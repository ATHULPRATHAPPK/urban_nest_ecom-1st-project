<!-- SweetAlert library -->
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<script>
    function showChangePasswordModal() {
        // Show SweetAlert confirmation dialog
        Swal.fire({
            title: 'Are you sure?',
            text: 'Do you want to change your password?',
            icon: 'question',
            showCancelButton: true,
            confirmButtonText: 'Yes',
            cancelButtonText: 'No',
        }).then((result) => {
            if (result.isConfirmed) {
                // If the user confirms, show the change password modal
                $('#changePasswordModal').modal('show');
            }
        });
    }

    function changePassword() {
        // Get current password and new password from form inputs
        const currentPassword = document.getElementById('currentPassword').value;
        const newPassword = document.getElementById('newPassword').value;

        // Make fetch request to change password endpoint
        fetch('/changePassword', {
            method: 'POST',
            headers: {
                'Content-Type': 'application/json',
            },
            body: JSON.stringify({ currentPassword, newPassword }),
        }).then(response => {
            if (response.ok) {
                // Password changed successfully, close the modal
                $('#changePasswordModal').modal('hide');
                Swal.fire({
                    title: 'Success',
                    text: 'Password changed successfully',
                    icon: 'success',
                });
                // Show success message
                document.getElementById('passwordCorrect').innerText = 'Password changed successfully';
            } else {
                // Error occurred, show error message
                response.json().then(data => {
                    // Display error message in div
                    document.getElementById('errorMessage').innerText = data.message;
                });
            }
        }).catch(error => {
            // Error occurred, show error message
            Swal.fire({
                title: 'Error',
                text: 'An error occurred. Please try again later.',
                icon: 'error',
            });
            console.error('Error:', error);
        });

        // Prevent form submission
        return false;
    }
</script>

