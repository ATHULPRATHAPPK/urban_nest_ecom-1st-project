                                                        }).then((result) => {
                                                            if (result.isConfirmed) {
                                                                // If user confirms, submit the form
                                                                $('#addressForm').submit();
                                                            }
                                                        });
                                                    }
                                                });
                                        
                                                // Handle form submission
                                                $('#addressForm').submit(function (event) {
                                                    event.preventDefault(); // Prevent the default form submission
                                        
                                                    if (validateForm()) {
                                                        // If form is valid, proceed with form submission via AJAX
                                                        var formData = {
                                                            name: $('#name').val(),
                                                            phone: $('#phone').val(),
                                                            building: $('#building').val(),
                                                            city: $('#city').val(),
                                                            district: $('#district').val(),
                                                            state: $('#state').val(),
                                                            pincode: $('#pincode').val()
                                                        };
                                        
                                                        // AJAX request to add address
                                                        $.ajax({
                                                            type: 'POST',
                                                            url: '/addAddress',
                                                            data: formData,
                                                            success: function (response) {
                                                                Swal.fire({
                                                                    title: 'Success!',
                                                                    text: response.message,
                                                                    icon: 'success',
                                                                    timer: 2000,
                                                                    timerProgressBar: true
                                                                }).then(() => {
                                                                    $('#exampleModalCenter').modal('hide');
                                                                    window.location.reload();
                                                                });
                                                            },
                                                            error: function (xhr, status, error) {
                                                                console.error('Error saving address:', error);
                                                            }
                                                        });
                                                    }
                                                });
                                        
                                                // Function to validate form fields
                                                function validateForm() {
                                                    var name = $('#name').val().trim();
                                                    var phone = $('#phone').val().trim();
                                                    var building = $('#building').val().trim();
                                                    var city = $('#city').val().trim();
                                                    var district = $('#district').val().trim();
                                                    var state = $('#state').val().trim();
                                                    var pincode = $('#pincode').val().trim();
                                        
                                                    // Regular expressions for validation
                                                    var namePattern = /^[A-Za-z\s]+$/;
                                                    var phonePattern = /^\d{10}$/;
                                                    var buildingPattern = /^[A-Za-z\s]+$/;
                                                    var cityPattern = /^[A-Za-z\s]+$/;
                                                    var districtPattern = /^[A-Za-z\s]+$/;
                                                    var statePattern = /^[A-Za-z\s]+$/;
                                                    var pincodePattern = /^\d{6}$/;
                                        
                                                    // Validation checks
                                                    if (!namePattern.test(name)) {
                                                        Swal.fire('Invalid Input', 'Name must contain only letters and spaces.', 'error');
                                                        return false;
                                                    }
                                        
                                                    if (!phonePattern.test(phone)) {
                                                        Swal.fire('Invalid Input', 'Phone number must contain 10 digits.', 'error');
                                                        return false;
                                                    }
                                        
                                                    if (!buildingPattern.test(building)) {
                                                        Swal.fire('Invalid Input', 'Building name must contain only letters and spaces.', 'error');
                                                        return false;
                                                    }
                                        
                                                    if (!cityPattern.test(city)) {
                                                        Swal.fire('Invalid Input', 'City must contain only letters and spaces.', 'error');
                                                        return false;
                                                    }
                                        
                                                    if (!districtPattern.test(district)) {
                                                        Swal.fire('Invalid Input', 'District must contain only letters and spaces.', 'error');
                                                        return false;
                                                    }
                                        
                                                    if (!statePattern.test(state)) {
                                                        Swal.fire('Invalid Input', 'State must contain only letters and spaces.', 'error');
                                                        return false;
                                                    }
                                        
                                                    if (!pincodePattern.test(pincode)) {
                                                        Swal.fire('Invalid Input', 'Pincode must contain 6 digits.', 'error');
                                                        return false;
                                                    }
                                        
                                                    return true; // Form is valid
                                                }
                                            });
                                        </script>
                                        
                                        
                                        
                                    </div>

