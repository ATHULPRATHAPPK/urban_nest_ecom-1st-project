            <script>
                document.addEventListener('DOMContentLoaded', function () {
                    // Find the "Sign out" button
                    const signOutBtn = document.getElementById('signOutBtn');

                    // Add click event listener to the "Sign out" button
                    signOutBtn.addEventListener('click', function () {
                        // Show SweetAlert confirmation dialog
                        Swal.fire({
                            title: 'Are you sure?',
                            text: "You will be logged out.",
                            icon: 'warning',
                            showCancelButton: true,
                            confirmButtonColor: '#3085d6',
                            cancelButtonColor: '#d33',
                            confirmButtonText: 'Yes, sign me out!'
                        }).then((result) => {
                            if (result.isConfirmed) {
                                // User confirmed, proceed with sign out action
                                // Replace the URL with the sign-out endpoint or action
                                window.location.href = "/user-logout"; // Example URL for sign-out action
                            }
                        });
                    });
                });
