                        <script>document.getElementById("collectSelectedDataButton").addEventListener("click", function() {
                            var selectedCategories = [];
                            var selectedColors = [];
                            var selectedBrands = [];
                        
                            var categoryCheckboxes = document.querySelectorAll('.custom-control-input[type="checkbox"][id^="category-"]:checked');
                            categoryCheckboxes.forEach(function(checkbox) {
                                var label = checkbox.parentElement.querySelector('.custom-control-label');
                                selectedCategories.push(label.textContent.trim());
                            });
                        
                            var colorCheckboxes = document.querySelectorAll('.custom-control-input[type="checkbox"][id^="color-"]:checked');
                            colorCheckboxes.forEach(function(checkbox) {
                                var label = checkbox.parentElement.querySelector('.custom-control-label');
                                selectedColors.push(label.textContent.trim());
                            });
                        
                            var brandCheckboxes = document.querySelectorAll('.custom-control-input[type="checkbox"][id^="brand-"]:checked');
                            brandCheckboxes.forEach(function(checkbox) {
                                var label = checkbox.parentElement.querySelector('.custom-control-label');
                                selectedBrands.push(label.textContent.trim());
                            });
                        
                            // Get the current URL
                            var currentUrl = window.location.href;
                        
                            // Parse the existing query parameters
                            var url = new URL(currentUrl);
                            var params = new URLSearchParams(url.search);
                        
                            // Update the query parameters with the selected values
                            params.set("categories", selectedCategories.join(","));
                            params.set("colors", selectedColors.join(","));
                            params.set("brands", selectedBrands.join(","));
                        
                            // Construct the new URL with updated query parameters
                            var newUrl = currentUrl.split("?")[0] + "?" + params.toString();
                        
                            // Redirect to the new URL
                            window.location.href = newUrl;
                        });
                        

                        </script>
                        
                        
                        
