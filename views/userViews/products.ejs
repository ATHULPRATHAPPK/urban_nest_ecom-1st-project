                                            <script>
                                                // Function to update the pagination query parameter without reloading the page
                                                function updatePaginationLaptop(pageNumber) {
                                                    // Get the current URL
                                                    let currentUrl = window.location.href;

                                                    // Parse the URL to get its components
                                                    let url = new URL(currentUrl);
                                                    let params = new URLSearchParams(url.search);

                                                    // Set the 'page' query parameter to the new page number
                                                    params.set('page', pageNumber);

                                                    // Remove the 'page' query parameter if pageNumber is 1
                                                    if (pageNumber === 1) {
                                                        params.delete('page');
                                                    }

                                                    // Construct the new URL with updated query parameters
                                                    let newUrl = `${url.origin}${url.pathname}?${params.toString()}${url.hash}`;

                                                    // Replace the current URL with the new URL
                                                    window.history.replaceState({ path: newUrl }, '', newUrl);

                                                    window.location.reload()
                                                }
                                            </script>


