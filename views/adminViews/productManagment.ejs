            <script>
              function openProductModal() {
                // Trigger the modal to open
                $('#productModal').modal('show');
              }
            
              function handleOptionSelection() {
                // Handle the selected option here (e.g., retrieve the selected value)
                var selectedOption = document.querySelector('input[name="productOption"]:checked');
                if (selectedOption) {
                  var selectedValue = selectedOption.value;
            
                  // Determine the destination URL based on the selected option
                  var destinationURL;
                  switch (selectedValue) {
                    case 'option1':
                      destinationURL = 'product/laptop';
                      break;
                    case 'option2':
                      destinationURL = 'product/mobile';
                      break;
                    case 'option3':
                      destinationURL = 'product/tablets';
                      break;
                    default:
                      // Default destination if none of the options match
                      destinationURL = 'default_destination.html';
                  }
            
                  // Navigate to the selected destination
                  window.location.href = destinationURL;
            
                  // Optionally, you can add further logic based on the selected option
            
                  // Close the modal (optional)
                  $('#productModal').modal('hide');
                }
              }
